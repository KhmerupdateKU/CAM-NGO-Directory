<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.camngo" version="2.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CAM NGO</name>
    <description>
        The app usefull for social. serve all people mostly freeland, ngo staff and student who seek for 
        volunteer at ngo by providing the phone email website address and map
    </description>
    <author email="n/a@n/a.com">
        NGO Team
    </author>
    <content src="index.html" />
    <icon src="res/ios/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
